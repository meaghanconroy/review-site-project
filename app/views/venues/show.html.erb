<%= @venue.name %>
<%= @venue.location %>
<%= @venue.ages %>
<%= @venue.capacity %>
<%= @venue.website %>
<img src="<%=@venue.photo%>">
<% @reviews.each do |review| %>
  <%= review.rating %>
  <%= review.body %>
  <%= review.created_at.strftime("%B %d %Y") %>
<% end %>
<%= link_to "Add a Review", new_venue_review_path(@venue) %>
